    "import csv\n",
    "with open('year2017.csv') as file:\n",
    "    data = list(csv.reader(file, skipinitialspace = True))\n",
    "    wounded = []\n",
    "    for values in data[1:]:\n",
    "        if values[-6] != '':\n",
    "            wounded.append(int(float(values[-6])))\n",
    "    print(sum(wounded))"
   ]
